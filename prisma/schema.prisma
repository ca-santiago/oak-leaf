generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Profile {
  id            String   @id @default(uuid())
  userId        String
  preferredName String?
  descrition    String?
  imageUri      String?
  updatedAt     DateTime @default(now())
}
